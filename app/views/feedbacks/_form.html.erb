<div class="box">
  <%= form_with model: @feedback, local: true do |form| %>
    <%= render 'shared/errors', resource: @feedback %>

    <div class="form-group">
      <!-- <%= form.label :name %><br> -->
      <%= form.text_field :name, placeholder: true %>
    </div>

    <div class="form-group">
      <!-- <%= form.label :email %><br/> -->
      <%= form.email_field :email, placeholder: true %>
    </div>

    <div class="form-group">
      <!-- <%= form.label :message %><br> -->
      <%= form.text_area :message, placeholder: true %>
    </div>
    <%= form.submit class: 'btn btn-primary' %>
  <% end %>
</div>

<tr>
  <td>
    <%= content_tag :span, test.title, class: 'test-title', data: { test_id: test.id } %>
    <%= render 'form_inline', test: test == @test ? @test : test %>
  </td>
  <td><%= test.category.title %></td>
  <td><%= test_level(test) %></td>
  <td><%= test.questions.count %></td>
  <td><%= test.author.full_name %></td>
  <td><%= link_to t('.edit'), edit_admin_test_path(test),
                              data: { test_id: test.id },
                              class: "form-inline-link white-link" %></td>
  <td><%= link_to t('.show'), admin_test_path(test), class: "white-link" %></td>
  <td><%= link_to t('.delete'), admin_test_path(test), class: "white-link", method: :delete, data: {confirm: t('.sure?')} %></td>
</tr>

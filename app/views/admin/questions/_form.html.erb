<div class="box">
  <%= form_with model: [:admin, @test, @question], local: true do |form| %>
    <%= render 'shared/errors', resource: @question %>

    <div class="field">
      <!-- <%= form.label :body %><br> -->
      <%= form.text_field :body, id: :question_body, placeholder: true %>
    </div>

    <p><%= form.submit %></p>
  <% end %>
</div>
